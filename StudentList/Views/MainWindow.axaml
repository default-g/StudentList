        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        mc:Ignorable="d" d:DesignWidth="500" d:DesignHeight="500"
		    Width="500" Height="500"
		    MaxWidth="500" MaxHeight="500"
		    MinWidth="500" MinHeight="500"
